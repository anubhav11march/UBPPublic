<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:dither="true" android:useLevel="false" android:visible="true" android:shape="rectangle">
            <solid android:color="@color/lightish_blue"/>
            <corners android:radius="30dp"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:dither="true" android:useLevel="false" android:visible="true" android:shape="rectangle">
            <solid android:color="@color/onboarding_blue"/>
            <corners android:radius="30dp"/>
        </shape>
    </item>
</selector>